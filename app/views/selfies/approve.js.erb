<% selfie = @selfie %>
$('.selfie_vote_block_<%= selfie.id %>').find('.upvote').html(<%= selfie.get_upvotes.size %>);
$('.selfie_vote_block_<%= selfie.id %>').find('.approve_button').addClass('vote_green');
$('.selfie_vote_block_<%= selfie.id %>').find('.downvote').html(<%= selfie.get_downvotes.size %>);
$('.selfie_vote_block_<%= selfie.id %>').find('.disapprove_button').removeClass('vote_red');