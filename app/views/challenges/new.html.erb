<% content_for :left_content do %>
	<%= render 'home/left_content' %>
<% end %>

<h3> Create a new Challenge </h3>
<%= form_for(@challenge, :html => { :class => 'form-horizontal'}) do |f| %>
	<%= render 'challenges/form', :f => f %>
	<div class="control-group">
	  <div class="controls">
	    <button type="submit" class="btn btn-primary">Add</button>
	  </div>
	</div>
<% end %>